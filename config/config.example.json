{
  "app": {
    "title": "My Home Dashboard",
    "theme": "dark",
    "defaultService": "home",
    "icon": "/images/logo.png"
  },
  "services": [
    {
      "id": "jellyfin",
      "name": "Jellyfin",
      "url": "https://jellyfin.example.com",
      "icon": "/images/jellyfin.png",
      "description": "Media server for movies and TV",
      "auth": {
        "type": "none"
      },
      "tags": ["media", "streaming"],
      "order": 1
    },
    {
      "id": "radarr",
      "name": "Radarr",
      "url": "https://radarr.example.com",
      "icon": "/images/radarr.png",
      "description": "Movie collection manager",
      "auth": {
        "type": "forms",
        "username": "admin",
        "password": "YOUR_PASSWORD_HERE"
      },
      "tags": ["media", "downloads"],
      "order": 2
    },
    {
      "id": "sonarr",
      "name": "Sonarr",
      "url": "https://sonarr.example.com",
      "icon": "/images/sonarr.png",
      "description": "TV show collection manager",
      "auth": {
        "type": "forms",
        "username": "admin",
        "password": "YOUR_PASSWORD_HERE"
      },
      "tags": ["media", "downloads"],
      "order": 2
    },
    {
      "id": "portainer",
      "name": "Portainer",
      "url": "https://portainer.example.com",
      "icon": "/images/portainer.png",
      "description": "Docker container management",
      "auth": {
        "type": "none"
      },
      "tags": ["infrastructure", "docker"],
      "order": 3,
      "openInNewTab": true
    },
    {
      "id": "grafana",
      "name": "Grafana",
      "url": "https://grafana.example.com",
      "icon": "/images/grafana.png",
      "description": "Metrics and monitoring dashboards",
      "auth": {
        "type": "none"
      },
      "tags": ["monitoring", "infrastructure"],
      "order": 4,
      "openInNewTab": true
    },
    {
      "id": "homeassistant",
      "name": "Home Assistant",
      "url": "https://homeassistant.example.com",
      "icon": "/images/homeassistant.png",
      "description": "Smart home automation",
      "auth": {
        "type": "bearer",
        "value": "YOUR_HA_LONG_LIVED_TOKEN_HERE"
      },
      "tags": ["home", "automation", "iot"],
      "order": 5
    },
    {
      "id": "sabnzbd",
      "name": "SABnzbd",
      "url": "https://sabnzbd.example.com",
      "icon": "/images/sabnzbd.png",
      "description": "Usenet downloader",
      "auth": {
        "type": "apikey",
        "header": "X-API-Key",
        "value": "YOUR_API_KEY_HERE"
      },
      "tags": ["downloads", "usenet"],
      "order": 6,
      "useProxy": true
    },
    {
      "id": "pihole",
      "name": "Pi-hole",
      "url": "https://pihole.example.com/admin",
      "icon": "/images/pihole.png",
      "description": "Network-wide ad blocking",
      "auth": {
        "type": "none"
      },
      "tags": ["network", "security", "dns"],
      "order": 7
    },
    {
      "id": "traefik",
      "name": "Traefik",
      "url": "https://traefik.example.com",
      "icon": "/images/traefik.png",
      "description": "Reverse proxy and load balancer",
      "auth": {
        "type": "basic",
        "username": "admin",
        "password": "YOUR_PASSWORD_HERE"
      },
      "tags": ["infrastructure", "networking", "proxy"],
      "order": 8,
      "openInNewTab": false
    }
  ]
}