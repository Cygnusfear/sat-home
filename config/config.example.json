      "auth": {
        "type": "none"
      },
      "tags": ["home", "automation", "iot"],
      "order": 6
    },
    {
      "id": "pihole",
      "name": "Pi-hole",
      "url": "https://pihole.example.com/admin",
      "icon": "/images/pihole.png",
      "description": "Network-wide ad blocking",
      "auth": {
        "type": "none"
      },
      "tags": ["network", "security", "dns"],
      "order": 7
    }
  ]
