{
  "// Authentication Note": "Most services use 'none' because they're behind a reverse proxy with SSO. Only use auth with useProxy:true when necessary (like SABnzbd)",
  "app": {
    "title": "My Home Dashboard",
    "theme": "dark",
    "defaultService": "home",
    "icon": "/images/logo.png",
    "style": "raycast"
  },
  "services": [
    {
      "id": "jellyfin",
      "name": "Jellyfin",
      "url": "https://jellyfin.example.com",
      "icon": "/images/jellyfin.png",
      "description": "Media server",
      "auth": {
        "type": "none"
      },
      "tags": ["media", "streaming"],
      "order": 1
    },
    {
      "id": "portainer",
      "name": "Portainer",
      "url": "https://portainer.example.com",
      "icon": "/images/portainer.png",
      "description": "Docker management",
      "auth": {
        "type": "none"
      },
      "tags": ["infrastructure", "docker"],
      "order": 2,
      "openInNewTab": true
    },
    {
      "id": "radarr",
      "name": "Radarr",
      "url": "https://radarr.example.com",
      "icon": "/images/radarr.png",
      "description": "Movie collection manager",
      "auth": {
        "type": "none"
      },
      "tags": ["media", "downloads"],
      "order": 3
    },
    {
      "id": "sonarr",
      "name": "Sonarr",
      "url": "https://sonarr.example.com",
      "icon": "/images/sonarr.png",
      "description": "TV show collection manager",
      "auth": {
        "type": "none"
      },
      "tags": ["media", "downloads"],
      "order": 3
    },
    {
      "// SABnzbd Note": "Uses proxy to handle iframe restrictions",
      "id": "sabnzbd",
      "name": "SABnzbd",
      "url": "https://sabnzbd.example.com",
      "icon": "/images/sabnzbd.png",
      "description": "Usenet downloader",
      "auth": {
        "type": "none"
      },
      "tags": ["downloads", "usenet"],
      "order": 4,
      "useProxy": true
    },
    {
      "id": "grafana",
      "name": "Grafana",
      "url": "https://grafana.example.com",
      "icon": "/images/grafana.png",
      "description": "Metrics and monitoring",
      "auth": {
        "type": "none"
      },
      "tags": ["monitoring", "infrastructure"],
